let lines = input.Split([| "\r\n"; "\n"|], StringSplitOptions.None)
type Direction = Up|Down|Left|Right

let mapToMove letter =
    match letter with 
    | 'U' -> Up
    | 'D' -> Down
    | 'R' -> Right
    | 'L' -> Left

let moveInDirection x direction =
    match (x,direction) with
    | (1,Up)
    | (2,Up)
    | (3,Up)
    | (3,Right)
    | (6,Right)
    | (9,Right)
    | (9, Down)
    | (8, Down)
    | (7, Down) 
    | (7, Left) 
    | (4, Left) 
    | (1, Left) as limits
        -> fst limits
    | (y,z) 
        -> match z with 
            | Up -> y - 3
            | Down -> y + 3 
            | Left -> y - 1
            | Right -> y + 1
            

let rec compute inp position (result : int list)=
    match inp with
    | [] -> result
    | head::tail ->
        let endPosition = 
            head 
            |> Seq.map mapToMove 
            |> Seq.fold (fun acc x -> moveInDirection acc x) position
        let newResult = endPosition :: result
        compute tail endPosition newResult


compute (lines |> Array.toList) 5 [] |> List.rev